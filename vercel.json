{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ],
    "env": {
      "DATABASE_URL": "@your_database_url"
    },
    "build": {
        "env": {
            "EXAMPLE_KEY": "value"
        },
        "use": "@vercel/node"
    }
  }
  